[Unit]
Description=Robolab renderer backend (stable)

[Service]
Type=exec
Restart=always
User=deploy
ExecStart=nodemon packages/robolab-jsServer/kotlin/robolab-jsServer.js -c ../backend.ini
WorkingDirectory=/home/deploy/web-projects/robolab-renderer/stable/backend

[Install]
WantedBy=multi-user.target
